import pandas as pd

data = {
    'post_id' : [101, 102, 103, 104, 105],
    'text' : [
        "I am feeling amazing today",
        "This is so fraustrating and unfair",
        "I just wanna cry",
        "WOW! That was unexpected",
        "Another day, another dollar."
    ],
    'emotions' : ['joy', 'angry', 'sadness','surprise', 'neutral']
}

df = pd.DataFrame(data)
print(df.head())

